<?xml version="1.0" ?>
<robot name="pumas_rover">
    <webots>
        <device reference="lidar" type="Lidar">
            <ros>
            <topicName>/pumas_rover/lidar</topicName>
            <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="camera" type="Camera">
            <ros>
            <topicName>/pumas_rover/camera</topicName>
            <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="webots_simul.my_rover_driver.MyRoverDriver" />

    </webots>
</robot>
